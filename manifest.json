{
  "manifest_version": 3,
  "name": "Soda Block",
  "version": "1.0.3",
  "description": "Block pop-up from https://old-stdportal.tdtu.edu.vn",

  "browser_specific_settings": {
    "gecko": {
      "id": "soda-block@ngochithuan",
      "data_collection_permissions": {
        "required": ["none"],
        "collects_personal_data": false,
        "collects_location": false,
        "collects_health_data": false,
        "collects_financial_data": false,
        "collects_authentication_info": false,
        "collects_messages": false,
        "collects_contacts": false,
        "collects_browsing_history": false,
        "collects_search_terms": false,
        "collects_user_activity": false,
        "collects_website_content": false
      }
    }
  },

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["https://old-stdportal.tdtu.edu.vn/*"],
      "js": ["main.js"]
    }
  ]
}

